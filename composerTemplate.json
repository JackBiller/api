{
	"name": "slim/slim-skeleton",
	"description": "A Slim Framework skeleton application for rapid development",
	"keywords": ["microframework", "rest", "router", "psr7"],
	"homepage": "http://github.com/slimphp/Slim-Skeleton",
	"license": "MIT",
	"authors": [
		{
			"name": "Jack Biller",
			"email": "jackbiller19@gmail.com",
			"homepage": "http://smallideasbigprojects.com/jack_biller/"
		}
	],
	"require": {
		"php": ">=5.6",
		"slim/slim": "^3.1",
		"slim/php-view": "^2.0",
		"phpmailer/phpmailer": "^6.2",
		"monolog/monolog": "^1.17"
	},
	"require-dev": {
		"phpunit/phpunit": ">5.0"
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true
	},
	"scripts": {
		"start": "php -S localhost:8080 -t public",
		"test": "phpunit"
	}
}
